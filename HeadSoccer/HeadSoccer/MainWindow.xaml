        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp" >
    <Grid>
        <Canvas x:Name="myCanvas" HorizontalAlignment="Left" Height="420" Margin="1,-1,-1,0" VerticalAlignment="Top" Width="792">
            <Image x:Name="Player" Height="100" Canvas.Left="78" Canvas.Top="241" Width="100" Source="player1.png"/>
            <Image x:Name="ground" Height="55" Canvas.Left="2" Canvas.Top="365" Width="792"/>
        </Canvas>
    </Grid>
